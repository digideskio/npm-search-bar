{
  "name": "npm-search-bar",
  "productName": "NPM Search",
  "version": "0.0.3",
  "main": "main.js",
  "license": "MIT",
  "author": "Joe McCann",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ --platform=darwin --arch=x64 --out=out --icon=./ns-icons.icns --overwrite",
    "package-all": "electron-packager ./ --all --out=out --icon=./ns-icons.icns --overwrite",
    "postpackage-all": "bin/zip.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joemccann/npm-search-bar"
  },
  "bugs": {
    "url": "https://github.com/joemccann/npm-search-bar/issues"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.2.7"
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "ejs": "^2.5.2",
    "electron-debug": "^1.0.1",
    "electron-json-storage": "^2.0.1",
    "jquery": "^3.1.1",
    "request": "^2.75.0"
  }
}
